	|	'READ' |  'READS' |  'REAL' |  'RECURSIVE' |  'REDO' |  'REF' |  'REFERENCES' |  'REFERENCING'
	|	'RELATIVE' |  'RELEASE' |  'REPEAT' |  'RESIGNAL' |  'RESTRICT' |  'RESULT' |  'RETURN'
	|	'RETURNS' |  'REVOKE' |  'RIGHT' |  'ROLE' |  'ROLLBACK' |  'ROLLUP' |  'ROUTINE'
