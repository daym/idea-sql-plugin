	| 	'AUTHORIZATION' embedded_authorization_identifier
		[  'FOR' 'STATIC' {  'ONLY' |  'AND' 'DYNAMIC' } ]
	| 	'SCHEMA' schema_name  'AUTHORIZATION' embedded_authorization_identifier
